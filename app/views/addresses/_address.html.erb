<div id="<%= dom_id address %>">
  <div class="container text-center">
    <div class="row justify-content-center">
      <div class="col-md-6">
        <div class="card">
          <p class="mt-3">
            <strong>University:</strong>
            <%= address.university.name %>
          </p>

          <p>
            <strong>Street:</strong>
            <%= address.street %>
          </p>

          <p>
            <strong>Number:</strong>
            <%= address.number %>
          </p>

          <p>
            <strong>Complement:</strong>
            <%= address.complement %>
          </p>

          <p>
            <strong>Neighborhood:</strong>
            <%= address.neighborhood %>
          </p>

          <p>
            <strong>City:</strong>
            <%= address.city %>
          </p>

          <p>
            <strong>State:</strong>
            <%= address.state %>
          </p>

          <p>
            <strong>Zipcode:</strong>
            <%= address.zipcode %>
          </p>

          <p>
            <strong>Country:</strong>
            <%= address.country %>
          </p>
        </div>
      </div>
    </div>
  </div>

</div>
