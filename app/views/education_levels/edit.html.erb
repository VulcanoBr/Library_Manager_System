

<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <div class="card">
        <div class="card-header">
          <h1 class="card-title">Edit Education Level</h1>
        </div>
        <div class="card-body">
          <%= render "form", education_level: @education_level %>
        </div>
        <div class="mt-4">
          <%= link_to "Show this education_level", @education_level, class: "btn btn-primary" %> |
          <%= link_to "Back to education levels", education_levels_path, class: "btn btn-secondary" %>
        </div>
      </div>
    </div>
  </div>
</div>